{
  "name": "autotree",
  "version": "1.0.0",
  "manifest_version": 3,
  "author": "Zhiping Lan",
  "description": "File navigator for github pull request",
  "homepage_url": "https://github.com/lanzhiping/autotree",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "content_scripts": [{
    "matches": [
      "https://*/*"
    ],
    "js": ["contentScript.js"]
  }]
}
